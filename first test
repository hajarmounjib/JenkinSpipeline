pipeline {
    agent any
    
    
        stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
            
        }

        stage('BUILD') {
            steps {
                echo 'conception'
            }
            
        }
    
        stage('TEST') {
            steps {
                echo 'testing'
            }
            
        }
 
        stage('DEPLOY') {
            steps {
                echo 'Mise en prod'
            }
            
        }
    }
}
